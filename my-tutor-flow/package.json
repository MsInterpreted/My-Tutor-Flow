{
  "name": "my-tutor-flow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "start": "vite",
    "test": "echo \"No tests specified\" && exit 0",
    "clean": "rm -rf dist node_modules/.vite",
    "prepare": "npm run type-check && npm run lint && npm run format:check",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "homepage": "https://dalz247.github.io/my-tutor-flow",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@mui/x-date-pickers": "^8.6.0",
    "@mui/x-date-pickers-pro": "^8.6.0",
    "@solana-mobile/mobile-wallet-adapter-protocol": "^2.2.2",
    "@solana-mobile/mobile-wallet-adapter-protocol-web3js": "^2.2.2",
    "@solana/wallet-standard-features": "^1.3.0",
    "@solana/wallet-standard-util": "^1.1.2",
    "dayjs": "^1.11.13",
    "firebase": "^11.9.1",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.3",
    "jspdf": "^3.0.1",
    "papaparse": "^5.5.3",
    "puppeteer": "^24.16.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-firebase-hooks": "^5.1.1",
    "react-native-quick-base64": "^2.2.1",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.15.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "prettier": "^3.6.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
